#include <stdio.h>

int main()
{
    int A,B,C,D,E;
    printf("Digite o valor de A: ");
    scanf("%d", &A);
    printf("Digite o valor de B: ");
    scanf("%d", &B);
    printf("Digite o valor de C: ");
    scanf("%d", &C);
    printf("Digite o valor de D: ");
    scanf("%d", &D);
    printf("Digite o valor de E: ");
    scanf("%d", &E);
    int areaTriangulo = (B * C) / 2; 
    printf("A área do triângulo é %d\n", areaTriangulo);
    int perimetroRetangulo = A + B + C + D;
    printf("O perímetro do retângulo é %d\n", perimetroRetangulo);
    float areaCirculo = 3.14 * (E * E);
    printf("A área do círculo é %f", areaCirculo);
}
